{
  "name": "jsdoc2doc",
  "version": "0.0.1",
  "description": "jsdoc to doc",
  "keywords": [
    "jsdoc",
    "doc",
    "tsdoc"
  ],
  "bin": {
    "jdoc": "./dist/cli.js"
  },
  "author": "clencat <2091927351@qq.com>",
  "license": "MIT",
  "packageManager": "pnpm@9.5.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a145789/jsdoc2doc.git"
  },
  "bugs": "https://github.com/a145789/jsdoc2doc/issues",
  "main": "./dist/index.js",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm run build && vr release",
    "lint": "npx @biomejs/biome lint ."
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.1",
    "@varlet/release": "^0.3.0",
    "tsup": "^8.3.5"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "cleye": "^1.3.2",
    "tinyexec": "^0.3.1",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.1.1",
    "rslog": "^1.2.3",
    "typescript": "^5.6.3"
  }
}
